   "source": [
    "# Write a Python program to get the Fibonacci series between 0 to 50"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "series=[0,1]\n",
    "num1=0\n",
    "num2=1\n",
    "sum=0\n",
    "while sum<50:\n",
    "    sum=num1+num2\n",
    "    num1=num2\n",
    "    num2=sum\n",
    "    series.append(sum)\n",
    " "
   ]
  },
